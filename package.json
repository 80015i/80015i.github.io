{
  "name": "boolsi github page",
  "version": "0.1.1",
  "description": "BoolSi GitHub page",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "predeploy": "npm run build",
    "start": "npm run build && ts-node src/index.ts",
    "deploy": "gh-pages -d build",
    "test": "nyc --reporter=json-summary --reporter=text mocha -r ts-node/register 'src/**/*.test.ts'",
    "main": "npm run build && npx webpack --config webpack.config.js",
    "build": "tsc",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/80015i/80015i.github.io/"
  },
  "nyc": {
    "include": "src",
    "exclude": [
      "src/presentation.ts",
      "*/**/*.test.ts"
    ]
  },
  "keywords": [],
  "author": "Mihailo Isakov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/80015i/80015i.github.io/"
  },
  "homepage": "https://github.com/80015i/80015i.github.io/",
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.13",
    "@types/sinon": "^9.0.10",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.15.0",
    "eslint-plugin-unused-imports": "^1.0.1",
    "imports-loader": "^4.0.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "sinon": "^9.2.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.1.3",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "animejs": "^3.2.1",
    "parse-svg-path": "^0.1.2",
    "snapsvg": "^0.5.1",
    "snapsvg-cjs": "^0.0.6",
    "svg-path-morph": "^1.0.2"
  }
}
